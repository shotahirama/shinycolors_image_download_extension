{
  "manifest_version": 2,
  "name": "ShinyColors ScreenShot",
  "version": "1",
  "description": "ScreenShot for IDOLM@STER ShinyColors",
  "permissions": [
    "tabs",
    "storage",
    "downloads"
  ],
  "background": {
    "scripts": ["bg.js"],
    "persistent": false
  },
  "page_action": {
  },
  "content_scripts": [
    {
      "matches": [
        "https://shinycolors.enza.fun/*"
      ],
      "js": [
        "content_test.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "windows": "Ctrl+Shift+S",
        "mac": "Ctrl+Shift+S",
        "linux": "Ctrl+Shift+S"
      }
    }
  }
}
